{
  "name": "@logkatt/pino-pretty",
  "version": "11.2.1",
  "description": "Prettifier for Pino log lines",
  "type": "commonjs",
  "main": "index.js",
  "types": "index.d.ts",
  "packageManager": "yarn@1.22.19",
  "bin": {
    "pino-pretty": "./bin.js"
  },
  "scripts": {
    "ci": "standard && tap --coverage-report=lcovonly && npm run test-types",
    "lint": "standard | snazzy",
    "test": "tap",
    "test-types": "tsc && tsd",
    "test:watch": "tap --no-coverage-report -w",
    "test:report": "tap --coverage-report=html"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/pinojs/pino-pretty.git"
  },
  "keywords": [
    "pino"
  ],
  "author": "James Sumners <james.sumners@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pinojs/pino-pretty/issues"
  },
  "homepage": "https://github.com/pinojs/pino-pretty#readme",
  "precommit": [
    "lint",
    "test"
  ],
  "dependencies": {
    "colorette": "2.0.20",
    "dateformat": "4.6.3",
    "fast-copy": "3.0.2",
    "fast-safe-stringify": "2.1.1",
    "help-me": "5.0.0",
    "joycon": "3.1.1",
    "json-bigint": "1.0.0",
    "minimist": "1.2.8",
    "on-exit-leak-free": "2.1.2",
    "pino-abstract-transport": "1.2.0",
    "pump": "3.0.0",
    "readable-stream": "4.5.2",
    "secure-json-parse": "2.7.0",
    "sonic-boom": "4.0.1",
    "strip-json-comments": "3.1.1"
  },
  "devDependencies": {
    "@types/node": "20.12.12",
    "fastbench": "1.0.1",
    "pino": "9.1.0",
    "pre-commit": "1.2.2",
    "rimraf": "3.0.2",
    "semver": "7.6.2",
    "snazzy": "^9.0.0",
    "standard": "17.1.0",
    "tap": "16.3.10",
    "tsd": "0.31.0",
    "typescript": "5.4.5"
  },
  "tsd": {
    "directory": "./test/types"
  }
}
